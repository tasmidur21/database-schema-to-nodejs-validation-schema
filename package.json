{
  "name": "@tasmidur/nodejs-validation-schema-rules",
  "version": "1.0.1",
  "private": false,
  "description": "Automatically generate initial Node.js validation schema rules from your database table schema. Use them as a foundation for refining and optimizing your validation rules as required",
  "bin": {
    "nodeVsr": "dist/cli.js"
  },
  "main": "dist/cli.js",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.hbs dist/",
    "start": "node dist/cli.js",
    "build": "yarn clean && tsc && yarn copy-files",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "node",
    "validatorjs",
    "schema",
    "nodejs-validation",
    "dynamic-schema",
    "dynamic-validation-rules-from-db"
  ],
  "author": "Md Tasmidur Rahman",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonyzach/usfm-validator.git"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "handlebars": "^4.7.8",
    "joi": "^17.4.0",
    "mysql2": "^3.7.0",
    "pg": "^8.7.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/pg": "^8.10.9",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
